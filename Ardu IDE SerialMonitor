void setup() {
  pinMode(LED_BUILTIN, OUTPUT);
  Serial.begin(115200);
}

void loop() {
  for(int i=0; i<9999; i++){ 
    digitalWrite(LED_BUILTIN, HIGH);  
    delay(1000);  
    Serial.print(i);         
    Serial.println(" kez lamba yandÄ±");
    digitalWrite(LED_BUILTIN, LOW);  
    delay(500); 
  }
}                   
